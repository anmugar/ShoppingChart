create table products (ProductID integer GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1) NOT NULL, UserID int, productName varchar(255), productDescription varchar (255), productPrice decimal(5,2),PRIMARY KEY (ProductID));
